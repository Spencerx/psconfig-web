    "tests": {
        "iperf3 TCP Test Between Testbeds": {
            "type": "throughput",
            "spec": {
                "duration": "PT20S",
                "source": "{% address[0] %}",
                "dest": "{% address[1] %}",
                "ip-version": 4
            }
        },
        "iperf3 UDP Test Between Testbeds": {
            "type": "throughput",
            "spec": {
                "duration": "PT10S",
                "source": "{% address[0] %}",
                "dest": "{% address[1] %}",
                "window-size": 1024000,
                "bandwidth": 50000000,
                "udp": true,
                "ip-version": 4
            }
        },
        "nuttcp UDP Test Between pScheduler Hosts": {
            "type": "throughput",
            "spec": {
                "duration": "PT10S",
                "source": "{% address[0] %}",
                "dest": "{% address[1] %}",
                "bandwidth": 50000000,
                "udp": true,
                "ip-version": 4,
                "interval": "PT1S"
            }
        },
        "Traceroute Between Testbeds": {
            "type": "trace",
            "spec": {
                "source": "{% address[0] %}",
                "dest": "{% address[1] %}",
                "sendwait": "PT5S",
                "wait": "PT30S",
                "ip-version": 4
            }
        },
        "Powstream Between Testbeds": {
            "type": "latencybg",
            "spec": {
                "source": "{% address[0] %}",
                "dest": "{% address[1] %}",
                "packet-padding": 0,
                "packet-interval": 0.1,
                "bucket-width": 0.001,
                "packet-count": 600,
                "ip-version": 4
            }
        },
        "Ping Between Testbeds": {
            "type": "rtt",
            "spec": {
                "source": "{% address[0] %}",
                "dest": "{% address[1] %}",
                "count": 10,
                "interval": "PT1S",
                "length": 1000,
                "ip-version": 4
            }
        },
        "Twamp test": {
            "type": "latency",
            "spec": {
                "source": "{% address[0] %}",
                "dest": "{% address[1] %}",
                "packet-interval": 0.1,
                "bucket-width": 0.001,
                "packet-count": 600,
                "ip-version": 4
            }
        }
